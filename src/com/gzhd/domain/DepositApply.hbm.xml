<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.gzhd.domain">
	<class name="DepositApply" table="tb_depositApply">

		<id name="id" column="id" type="string" length="32"><!-- id 表示主键 -->
			<generator class="uuid"></generator>  <!-- 主键的生成方式 -->
		</id>

		<property name="userId" type="string">
			<column name="userId" length="32">
				<comment>申请人id</comment>
			</column>
		</property>

         <property name="money" type="string">
			<column name="money" length="30">
				<comment>金额</comment>
			</column>
		</property>
		<property name="bankNum" type="string">
			<column name="bankNum" length="30">
				<comment>银行卡号</comment>
			</column>
		</property>

		<property name="applyTime" type="string">
			<column name="applyTime" length="30">
				<comment>申请时间</comment>
			</column>
		</property>

		<property name="status" type="string">
			<column name="status" length="30">
				<comment>状态</comment>
			</column>
		</property>
        
        <property name="handletime" type="string">
			<column name="handletime" length="30">
				<comment>处理时间</comment>
			</column>
		</property>

	</class>


</hibernate-mapping>